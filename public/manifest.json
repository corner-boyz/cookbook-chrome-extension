{
  "short_name": "Cook Book",
  "name": "Cook Book Chrome Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Cook Book"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0.0",
  "permissions": ["<all_urls>",
    "storage",
    "tabs"
  ]
}
